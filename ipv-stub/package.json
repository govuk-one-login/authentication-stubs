{
  "description": "Authentication stub for IPV",
  "license": "MIT",
  "main": "index.js",
  "name": "auth-ipv-stub",
  "version": "1.0.0",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.876.0",
    "@aws-sdk/lib-dynamodb": "^3.876.0",
    "esbuild": "0.25.2",
    "jose": "5.10.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^10.0.0"
  },
  "scripts": {
    "build": "sam build --template template.yaml --build-dir build/ipv",
    "clean": "rm -rf build/ipv & rm -rf build/build.toml",
    "start:local": "sam local start-api --template build/ipv/template.yaml --env-vars parameters.json -p 3000",
    "compile": "tsc",
    "encryptSampleRequest": "node scripts/encrypt-message-locally.mjs",
    "pretty": "prettier --write '**/*.ts' '**/*.js'",
    "pretty-check": "prettier --check '**/*.ts' '**/*.js'",
    "lint": "eslint . --fix",
    "test-unit": "mocha"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@types/aws-lambda": "8.10.152",
    "@types/aws-sdk": "2.7.4",
    "@types/chai": "^4.3.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.14.0",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.31.0",
    "chai": "^4.3.6",
    "eslint": "^9.34.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "globals": "^16.3.0",
    "mocha": "11.7.1",
    "prettier": "3.6.2",
    "sinon": "^21.0.0",
    "ts-node": "^10.9.2",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=20.0.0 <21"
  },
  "mocha": {
    "diff": true,
    "extension": [
      "js",
      "ts"
    ],
    "timeout": 20000,
    "recursive": true,
    "require": [
      "ts-node/register"
    ],
    "exit": true
  }
}
